<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <LangVersion>default</LangVersion>
    <Version>3.0.0.0</Version>
    <Deterministic>false</Deterministic>
    <AssemblyVersion>$(Version)</AssemblyVersion>
    <FileVersion>$(Version)</FileVersion>
    <Authors>Markus Schmidt</Authors>
    <Company>Markus Schmidt (Pro)</Company>
    <Product>CLArgs - Command Line Arguments</Product>
    <Copyright>Copyright (c) 2024, Markus Schmidt (Pro), all rights reserved.</Copyright>
    <Nullable>enable</Nullable>

  </PropertyGroup>

  <ItemGroup>
    <None Remove="cs.bat" />
    <None Remove="sln.profile" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="cs.bat" />
    <Content Include="sln.profile">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <None Remove="flutter.profile" />
    <Content Include="flutter.profile">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="8.0.0" />
    <PackageReference Include="MSPro.CLArgs" Version="2.2312.16" />
  </ItemGroup>
</Project>
